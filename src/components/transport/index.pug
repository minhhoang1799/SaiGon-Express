section.transport-4.main-line
	.container
		.wrapper.main-child
			h3.main-title công thức
			p.decs Tùy thuộc và công trình chuyển nhà mà giá dịch vụ chuyển dọn nhà, văn phòng Saigon Express có thể thay đổi khác nhau và được tính theo công thức sau:  
			ul.nav-transport
				li
					p Chuyển nhà, văn phòng, kho xưởng chọn gói
				li
					p Cước thuê xe tải
				li
					p Phí bốc xếp
				li
					p Chi phí khác (nếu có)
		.wrapper.main-child
			h3.main-title giá cước thuê xe vận chuyển nhà, văn phòng - saigon express
			.table-wrapper
				table
					tr
						th loại xe
						th giá mở cửa - 10km đầu
						th từ km thứ 11 đến 44
						th từ km thứ 45
						th thời gian chờ
					- for(i=0;i<5;i++)
						tr
							td Xe tải nhỏ 500kg
							td 250.000VNĐ
							td 13.000VNĐ/Km
							td 12.000VNĐ/Km
							td 70.000VNĐ/Giờ
		.wrapper.main-child
			h3.main-title phí bốc xếp
			.table-wrapper
				table
					tr
						th loại xe
						th phí bốc xếp 2 đầu
					- for(i=0;i<5;i++)
						tr
							td Xe tải nhỏ 500kg
							td 140.000VNĐ
		.wrapper.main-child
			h3.main-title chi phí chuyển nhà khác nếu có
			.table-wrapper
				table
					tr
						th loại xe
						th chi phí
					- for(i=0;i<5;i++)
						tr
							td Thùng Carton (60x40x35 cm)
							td 18.000VNĐ/Thùng
		.wrapper.main-child
			h3.main-title một số câu hỏi liên quan đến dịch vụ chuyển nhà thường gặp
			.list-question.main-line
				ul
				- for(i=0;i<6;i++)
					li
						.question-wrapper
							p.question 01. Xe tải chở hàng gồm những loại nào?
							.reply
								p Nếu có người hỗ trợ, hoặc có đủ thời gian, bạn có thể tự đóng gói đồ đạc và chỉ thuê xe chuyển nhà. Như vậy sẽ tiết kiệm chi phí hơn.
		.wrapper.main-child
			h3.main-title một số hình ảnh thực tế các loại xe và hình ảnh chuyển nhà chọn gói
			.container
				.row.list-image
					.col(class="w-full lg:w-1/2")
						.image-full
							img(src="./img/transport/2.png", alt="")
					.col(class="w-full lg:w-1/2")
						.image-full
							img(src="./img/transport/2.png", alt="")
					.col(class="w-full lg:w-1/3")
						.image-child
							img(src="./img/transport/3.png", alt="")
					.col(class="w-full lg:w-1/3")
						.image-child
							img(src="./img/transport/3.png", alt="")
					.col(class="w-full lg:w-1/3")
						.image-child
							img(src="./img/transport/3.png", alt="")
					.col.w-full
